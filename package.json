{
  "name": "curriculo.vivo",
  "version": "1.0.0",
  "author": "lucasferreiralimax (https://github.com/lucasferreiralimax)",
  "repository": "https://github.com/criar-art/curriculo.vivo",
  "bugs": {
    "url": "https://github.com/criar-art/curriculo.vivo/issues"
  },
  "scripts": {
    "start": "vite --host localhost --port 9999",
    "dev": "vite --host localhost --port 9999",
    "serve": "vite --host localhost --port 9999 preview",
    "build": "vite build",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "preview": "vite --host localhost preview --port 5050",
    "readme": "node -r esm readme.js",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'"
  },
  "dependencies": {
    "@mdi/font": "^7.0.96",
    "crypto-js": "^4.1.1",
    "firebase": "^9.10.0",
    "lodash": "^4.17.21",
    "pinia": "^2.0.22",
    "roboto-fontface": "*",
    "stylus": "^0.59.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vuetify": "^3.0.0-beta.4",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.1.4",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vuetify/vite-plugin": "^1.0.0-alpha.11",
    "cypress": "^10.3.1",
    "eslint": "^8.20.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "esm": "^3.2.25",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "pug": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.0.3",
    "vitest": "^0.23.4",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vuetify-loader": "^2.0.0-alpha.0"
  }
}
